{
  "name": "you-music",
  "version": "1.0.0",
  "description": "aplicaci√≥n de musica gratiuta multiplataforma",
  "main": "./app/index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "Nakato",
  "license": "MIT",
  "build": {
    "appId": "you-music-app001",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "dependencies": {
    "axios": "^0.21.1",
    "firebase-admin": "^9.11.0",
    "ytsr": "^3.5.0"
  },
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7"
  }
}
